pragma solidity 0.8.22;

import {Test, console} from "forge-std/Test.sol";
import {MoodNFT} from "../../src/MoodNFT.sol";
import {DeployMoodNFT} from "../../scripts/DeployMoodNFT.s.sol";

//
// import {Vm} from "forge-std/src/Vm.sol";

contract TestDeployMoodNFTIntegration is Test {
    MoodNFT public moodNFT;
    DeployMoodNFT public deployer;
    address public USER = makeAddr("user");
    string public constant HAAPY_SVG_IMAGE_URL =
        "data:image/svg+xml;base64,";
    string public constant SAD_SVG_IMAGE_URL =
        "data:image/svg+xml;base64,";
    string public constant SAD_SVG_URL =
        "data:application/json;base64,";
    string public constant HAPPY_SVG_URL =
        " data:application/json;base64,";

    // Hash:"0xf90ae014c41cb48231e1f02c314087ff9c479133697911d25c5fe231e237dd14";

    function setUp() public {
        deployer = new DeployMoodNFT();
        moodNFT = deployer.run();
    }

    function testViewTokenURIIntegration() external {
        //Arrange
        vm.prank(USER);
        //Act
        moodNFT.mint();
        console.log(moodNFT.tokenURI(0));
    }

    function testFlipTokenToSad() external {
        //Wanted to try this it didn't work
        // vm.startPrank();
        // vm.stopPrank();

        //Arrange/ACT
        vm.prank(USER);
        moodNFT.mint();

        vm.prank(USER);
        moodNFT.flipMood(1);

        console.log(moodNFT.tokenURI(0));
        console.log(SAD_SVG_URL);

        //assert
        assertEq(
            keccak256(abi.encodePacked(moodNFT.tokenURI(0))),
            keccak256(abi.encodePacked(SAD_SVG_URL))
        );
    }
}
